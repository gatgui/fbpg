
x = 1.0
y = -x

println("x = " + x)
println("y = " + y)

if x >= y and y < 0  then
  println("x>=y and y<0")
else
  println("flawed logic")
end

if x >= y and not y >= 0 then
  println("x>=y and y<0")
else
  println("flawed logic")
end

if x < 0 or y < 0 then
  println("x<0 or y<0")
else
  println("flawed logic")
end

if not x < 0 or y > 0 then
  println("not x<0 or y>0")
else
  println("flawed logic")
end

if not (x>0 or y<0) then
  println("flawed logic")
else
  println("x>0 and y<0")
end
