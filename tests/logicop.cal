
x = 1.0
y = -x

print("x = " .. x .. "\n")
print("y = " .. y .. "\n")

if x >= y and y < 0  then
  print("x>=y and y<0\n")
else
  print("flawed logic\n")
end

if x >= y and not y >= 0 then
  print("x>=y and y<0\n")
else
  print("flawed logic\n")
end

if x < 0 or y < 0 then
  print("x<0 or y<0\n")
else
  print("flawed logic\n")
end

if not x < 0 or y > 0 then
  print("not x<0 or y>0\n")
else
  print("flawed logic\n")
end

if not (x>0 or y<0) then
  print("flawed logic\n")
else
  print("x>0 and y<0\n")
end
